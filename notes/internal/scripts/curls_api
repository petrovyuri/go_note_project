# Создание новой заметки
curl -X POST "http://localhost:8103/notes/note" \
     -H "Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJleHAiOjE3NTE5NjQxMDMsImlhdCI6MTc1MTg3NzcwMywiaWQiOjMsInR5cGUiOiJhY2Nlc3NUb2tlbiJ9.-rU25KHd6GUUIZX25J2ymwoTc8rD88sXKY1FXHOSzeM" \
     -H "Content-Type: application/json" \
     -w "\nStatus: %{http_code}\n"


# Получение списка всех заметок
curl -X GET "http://localhost:8103/notes/notes" \
     -H "Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJleHAiOjE3NTE5NjQxMDMsImlhdCI6MTc1MTg3NzcwMywiaWQiOjMsInR5cGUiOiJhY2Nlc3NUb2tlbiJ9.-rU25KHd6GUUIZX25J2ymwoTc8rD88sXKY1FXHOSzeM" \
     -H "Content-Type: application/json" \
     -w "\nStatus: %{http_code}\n" 

# Получение заметки по ID
curl -X GET "http://localhost:8103/notes/note/1" \
     -H "Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJleHAiOjE3NTE5NjQxMDMsImlhdCI6MTc1MTg3NzcwMywiaWQiOjMsInR5cGUiOiJhY2Nlc3NUb2tlbiJ9.-rU25KHd6GUUIZX25J2ymwoTc8rD88sXKY1FXHOSzeM" \
     -H "Content-Type: application/json" \
     -w "\nStatus: %{http_code}\n"

#  Редактирование заметки по ID
curl -X PUT "http://localhost:8103/notes/note/1" \
     -H "Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJleHAiOjE3NTE5NjQxMDMsImlhdCI6MTc1MTg3NzcwMywiaWQiOjMsInR5cGUiOiJhY2Nlc3NUb2tlbiJ9.-rU25KHd6GUUIZX25J2ymwoTc8rD88sXKY1FXHOSzeM" \
     -H "Content-Type: application/json" \
     -w "\nStatus: %{http_code}\n"
     
# Удаление заметки по ID
curl -X DELETE "http://localhost:8103/notes/note/1" \
     -H "Authorization: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJleHAiOjE3NTE5NjQxMDMsImlhdCI6MTc1MTg3NzcwMywiaWQiOjMsInR5cGUiOiJhY2Nlc3NUb2tlbiJ9.-rU25KHd6GUUIZX25J2ymwoTc8rD88sXKY1FXHOSzeM" \
     -H "Content-Type: application/json" \
     -w "\nStatus: %{http_code}\n" 